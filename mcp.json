{
  "filesystem": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-filesystem", "{filesystem_path}"],
    "transport": "stdio"
  },
  "duckduckgo": {
    "command": "uvx",
    "args": ["duckduckgo-mcp-server"],
    "transport": "stdio"
  },
  "fetch": {
    "command": "uvx",
    "args": ["mcp-server-fetch"],
    "transport": "stdio"
  },
  "blender": {
    "command": "uvx",
    "args": ["blender-mcp"],
	"transport": "stdio"
  }  
}